class Program
    {
        String MaxStr(List<string>str)
        {
            int max = 0;
            string zbor = str[0];
            for (int i = 0; i < str.Count; i++)
            {
                List<char> karakteri = new List<char>();
                for (int j = 0; j < str[i].Length; j++)
                {
                    karakteri.Add(str[i][j]);
                }
            }
            for (int i = 0; i < str.Count; i++)
            {
                if (str[i].Length > max)
                {
                    max = str[i].Length;
                    zbor = str[i];
                }
            }
            return zbor;
        }
        static void Main(string[] args)
        {
            List<string> str = new List<string>() { "Den", "Mesec", "Godinite", "Nedela" };
            string maxStr;
            Program obj = new Program();
            maxStr = obj.MaxStr(str);
            Console.WriteLine(" Najdolgiot memoriran string e " + maxStr);
        }
    }
}
