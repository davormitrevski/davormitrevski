 static bool Fibonaci(string ime)
        {
            int prv = 0;
            int vtor = 1;
            bool pripagja = false;
            int tret = prv + vtor;
            if (tret == ime.Length)
            {
                pripagja = true;
            }
            while (tret < ime.Length)
            {
                tret = prv + vtor;
                if (tret == ime.Length)
                {
                    pripagja = true;
                    break;
                }
                prv = vtor;
                vtor = tret;
            }
            return pripagja;
        }
        static void Main(string[] args)
        {
            List<string> iminjaF = new List<string>() { "Kire", "Bojan", "Stojan", "Aleksandar" };
            List<string> fibonaci = new List<string>();
            bool fib;
            for (int i = 0; i < iminjaF.Count; i++)
            {
                fib = Fibonaci(iminjaF[i]);
                if (fib == true)
                {
                    fibonaci.Add(iminjaF[i]);
                }
                
            }
            for (int i = 0; i < fibonaci.Count; i++)
            {
                Console.WriteLine(fibonaci);
            }
        }
    }
}
