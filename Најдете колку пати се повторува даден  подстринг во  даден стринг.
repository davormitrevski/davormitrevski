class Program
    {
        int KolkuPatiSePovtoruva(string str, string podstr)
        {
            int patisepovtoruva = 0;
            for (int i = 0; i < str.Length - podstr.Length + 1; i++)
            {
                int brojac = 0;
                for (int j = 0; j < podstr.Length; j++)
                {
                    if (str[i + j] == podstr[j])
                        brojac++;
                }
                if (brojac == podstr.Length)
                    patisepovtoruva++;
            }
            return patisepovtoruva;
        }
        static void Main(string[] args)
        {
            string str = "asdfeeasdfrgrasdo";
            string podstr = "asd";
            Program obj = new Program();
            int a;
            a = obj.KolkuPatiSePovtoruva(str, podstr);
            Console.WriteLine(" Podstringot " + podstr + " se povtoruva " + a + " pati. ");
        }
    }
}
