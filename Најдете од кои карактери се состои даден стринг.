class Program
    {
        List<char> KarakteriOdString(string str)
        {
            List<char> karakter = new List<char>();
            for (int i = 0; i < str.Length; i++)
            {
                karakter.Add(str[i]);
            }
            return karakter;
        }
        static void Main(string[] args)
        {
            string str = "Nedela";
            List<char> karakter = new List<char>();
            Program obj = new Program();
            karakter = obj.KarakteriOdString(str);
            Console.WriteLine("Dadeniot string " + str +  " se sostoi od slednite karakteri : ");
            for (int i = 0; i < karakter.Count; i++)
            {
                Console.WriteLine(karakter[i]);
                
            }
        }
    }
}
