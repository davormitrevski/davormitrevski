 class Program
    {
        List<char> Karakteri(string str)
        {
            List<char> karakter = new List<char>();
            for (int i = 0; i < str.Length; i++)
            {
                bool ima = false;
                for (int j = 0; j < karakter.Count; j++)
                {
                    if (str[i] == karakter[j])
                        ima = true;
                }
                       
                        {
                            karakter.Add(str[i]);
                        }
                    
                }
            return karakter;
            }
        List<int>PatiNaPovtoruvajne(List<char>karakter, string str)
        {
            List<int> povtoruvajne = new List<int>();
            for (int i = 0; i < karakter.Count; i++)
            {
                int brojac = 0;
                for (int j = 0; j < str.Length; j++)
                {
                    if (karakter[i] == str[j])
                    {
                        brojac = brojac + 1;
                    }
                }
                povtoruvajne.Add(brojac);
            }
            return povtoruvajne;
        }
        

        static void Main(string[] args)
        {
            string str = "asdasfkamfca";
            List<char> karakter = new List<char>();
            Program obj = new Program();
            karakter = obj.Karakteri(str);
            List<int> povtoruvajne = new List<int>();
            povtoruvajne = obj.PatiNaPovtoruvajne(karakter, str);
            for (int i = 0; i < karakter.Count; i++)
            {
                Console.WriteLine(" Karakterot " + karakter[i] + " se povtoruva " + povtoruvajne[i] + " pati ");
            }
        }
    }
}
