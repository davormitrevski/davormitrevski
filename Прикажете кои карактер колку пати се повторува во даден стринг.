class Program
    {
        static void Main(string[] args)
        {
            string str = "abceabadfa";
            char c = 'a';
            int brojac = 0;
            for (int i = 0; i < str.Length; i++)
            {
                if (str[i] == c)
                {
                    brojac = brojac + 1;
                }

            }
            Console.WriteLine("Karakterot se povtoruva " + brojac + " pati ");

        }
    }
}
